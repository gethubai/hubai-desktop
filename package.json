{
  "version": "0.1.0-alpha.18",
  "description": "The central place for all AIs",
  "keywords": [
    "hubai",
    "ai",
    "hub",
    "aihub"
  ],
  "homepage": "https://hubai.app",
  "bugs": {
    "url": "https://github.com/gethubai/hubai-desktop/issues"
  },
  "repository": "https://github.com/gethubai/hubai-desktop",
  "license": "MIT",
  "author": {
    "name": "Matheus Diniz",
    "email": "matheus.diniz@hubai.app",
    "url": "https://github.com/mtsdnz"
  },
  "contributors": [
    {
      "name": "Matheus Diniz",
      "email": "matheus.diniz@hubai.app",
      "url": "https://github.com/mtsdnz"
    }
  ],
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "test-build": "npx cross-env SENTRY_NO_SOURCE_MAPS=true npm run build",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "release": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish always",
    "release-debug": "npx cross-env DEBUG_PROD=true npm run release",
    "package-debug": "npx cross-env DEBUG_PROD=true npm run package",
    "package-win-debug": "npx cross-env DEBUG_PROD=true npm run package -- --windows",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development env-cmd electronmon -r ts-node/register/transpile-only -r tsconfig-paths/register .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist",
      ".erb/dll"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@dtinsight/dt-utils": "^1.3.1",
    "@hcaptcha/react-hcaptcha": "^1.10.1",
    "@hubai/brain-sdk": "^1.0.9",
    "@hubai/core": "^0.0.29",
    "@rjsf/core": "^5.18.3",
    "@rjsf/utils": "^5.18.3",
    "@rjsf/validator-ajv8": "^5.18.3",
    "@sentry/electron": "^4.24.0",
    "@vscode/codicons": "^0.0.35",
    "applicationinsights": "^3.0.1",
    "axios": "^1.6.8",
    "buffer": "^6.0.3",
    "electron-auth0-login": "^2.0.5",
    "electron-context-menu": "^3.6.1",
    "electron-debug": "^3.2.0",
    "electron-dl": "^3.5.2",
    "electron-log": "^5.1.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.31.0",
    "mqtt": "^5.5.5",
    "multer": "^1.4.5-lts.1",
    "node-stream-zip": "^1.15.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "^18.3.1",
    "react-h5-audio-player": "^3.9.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.23.0",
    "react-scrollbars-custom": "^4.1.1",
    "react-simple-keyboard": "^3.7.116",
    "react-toastify": "^10.0.5",
    "reflect-metadata": "^0.2.2",
    "regexparam": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "semver": "^7.6.1",
    "styled-components": "^6.1.10",
    "tsyringe": "^4.8.0",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "@electron/notarize": "^2.3.0",
    "@electron/rebuild": "^3.6.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
    "@sentry/webpack-plugin": "^2.16.1",
    "@svgr/webpack": "^8.0.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.7",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/multer": "^1.4.11",
    "@types/node": "20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^8.1.0",
    "core-js": "^3.37.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "detect-port": "^1.6.1",
    "dotenv-webpack": "^8.1.0",
    "electron": "27.3.11",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "node-abi": "^3.62.0",
    "prettier": "^3.2.5",
    "react-refresh": "^0.14.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^5.0.1",
    "sass": "^1.76.0",
    "sass-loader": "^14.2.1",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.9.0"
  },
  "build": {
    "productName": "HubAI",
    "appId": "com.hubai.app",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json",
      "!**/node_modules/realm/react-native"
    ],
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "notarize": {
        "teamId": "5KNK52N242"
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "icon": "assets/icon.icns",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development",
      "icon": "assets/icon.png"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "gethubai",
      "repo": "hubai-desktop",
      "releaseType": "release"
    }
  },
  "collective": {
    "url": "https://opencollective.com/hubai-desktop"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
